{
  "name": "pooltracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.12.1",
    "lodash": "^4.17.5",
    "node-sass-chokidar": "^1.2.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-icons": "^2.2.7",
    "react-scripts": "1.1.4",
    "react-table": "^6.8.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/index.scss -o src/styles/css",
    "watch-css": "npm run build-css & node-sass-chokidar src/styles/index.scss -o src/styles/css --watch --recursive",
    "start": "npm run watch-css | react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
